#include<iostream>
using namespace std;
int main()
{
  unsigned char str;

  std::cout << "Input an unsigned character string for hashing" << '\n';
  std::cin >>str;
  std::cout << "Hash: " << hash(&str) << '\n';
}

unsigned long hash(unsigned char *str)
{
  unsigned long hash=5381;
  int c;
  while ((c=*str++)) hash=((hash << 5) + hash) + c;
  //hash*33+c
  return hash;
}
//function returns index
//has to hash from key type to integer type
// hash table is simply an array of linked lists
//hash table has keys and elements

class hash
{
public:
  get_offset()
{

};
set_offset()
{
  int offset;
  hash%table=offset;

  for(offset>5381)
  {
    std::cout << "There is not enough space in hashtable" << std::endl;
  }
};


};
